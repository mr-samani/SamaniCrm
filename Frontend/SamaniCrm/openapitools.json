{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.12.0",
    "generators": {
      "v2.0": {
        "generatorName": "typescript-angular",
        "inputSpec": "https://localhost:44301/swagger/v1/swagger.json",
        "output": "src/shared/service-proxies",
        "additionalProperties": {
          "ngVersion": "19.2.0",
          "providedInRoot": true,
          "stringEnums": true,
          "modelSuffix": "",
          "serviceSuffix": "ServiceProxy",
          "fileNaming": "kebab-case",
          "enumNameSuffix": "Enum",
          "withInterfaces": false,
          "useSingleRequestParameter": false,
          "skipFormModel": true
        }
      }
    }
  }
}
