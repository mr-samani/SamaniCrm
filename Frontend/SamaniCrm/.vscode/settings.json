{
    "files.exclude": {
        ".angular":true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "node_modules": true,
        "bin": true,
        "obj": true,
        "dist": true,
        "Properties": true,
        ".vs": true,
        "*.csproj": true,
        "*.sln": true,
        "*.csproj.user": true,
        "*.suo": true,
    },
    "ngFileGenerator.baseComponentClassName": "AppComponentBase",
    "ngFileGenerator.baseComponentPath": "@app/app-component-base",
    "sqltools.connections": [
        {
            "connectionTimeout": 15,
            "mssqlOptions": {
                "appName": "SQLTools",
                "useUTC": true,
                "encrypt": true,
                "trustServerCertificate": false
            },
            "previewLimit": 50,
            "server": "localhost",
            "port": 1433,
            "driver": "MSSQL",
            "name": "m",
            "username": "sa"
        }
    ]
}